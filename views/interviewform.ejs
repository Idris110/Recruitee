<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/test.css">
</head>

<body>
    <div class="mainCont">
        <form  action="/calendar" method="post">
            <!-- 2 column grid layout with text inputs for the first and last names -->
            

            <!-- Email input -->
            <div class="form-outline mb-4">
                <input autocomplete="off" type="text" name="summary" id="form3Example3" class="form-control" />
                <label class="form-label" for="form3Example3">Summary</label>
            </div>
            <div class="form-outline mb-4">
                <input autocomplete="off" type="text" name="description" id="form3Example3" class="form-control" />
                <label class="form-label" for="form3Example3">Location</label>
            </div>
            <div class="form-outline mb-4">
                <input autocomplete="off" type="email" name="email" id="form3Example3" class="form-control" />
                <label class="form-label" for="form3Example3">E-mail</label>
            </div>

            <!-- Password input -->
            <div class="form-outline mb-4">
                <input autocomplete="off" type="datetime-local" name="eventStarttime" id="form3Example4" class="form-control" />
                <label class="form-label" for="form3Example4">Event Start Time</label>
            </div>
            <div class="form-outline mb-4">
                <input autocomplete="off" type="datetime-local" name="eventEndtime" id="form3Example4" class="form-control" />
                <label class="form-label" for="form3Example4">Event End Time</label>
            </div>

            <!-- Submit button -->
            <button type="submit" class="btn btn-primary btn-block mb-4">Sign up</button>

        </form>
    </div>
</body>
</head>